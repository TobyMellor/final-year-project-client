{
  "name": "final-year-project-client",
  "version": "1.0.0",
  "description": "The front-end for my Final Year Project at Loughborough University",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "npm run clean && npm run webpack",
    "clean": "rm -rf dist && mkdir -p dist",
    "lint": "node ./node_modules/.bin/tslint --config tslint.json --project tsconfig.json 'src/typescript/**/*.ts{,x}'",
    "local": "webpack-dev-server --host 0.0.0.0 --port 8888 --mode=\"development\" --watch-content-base --progress --colors --content-base dist/",
    "prod": "npm run webpack && npm start",
    "test": "jest",
    "webpack": "node ./node_modules/.bin/webpack",
    "server": "npm run build && cd server && node app.js"
  },
  "repository": {
    "type": "git",
